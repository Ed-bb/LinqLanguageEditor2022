<UserControl x:Class="LinqLanguageEditor2022.Options.CodeStyleSpacingOptions"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LinqLanguageEditor2022.Options"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <CheckBox x:Name="cbInsertSpaceBetweenMethodNameOpenParenthesis" Content="Insert space between method name and its opening parenthesis" HorizontalAlignment="Left" Margin="30,36,0,0" VerticalAlignment="Top" IsThreeState="False" Checked="cbInsertSpaceBetweenMethodNameOpenParenthesis_Checked"/>
        <CheckBox x:Name="cbInsertSpaceInParameterlistParentheses" Content="Insert space within parameter list parentheses" HorizontalAlignment="Left" Margin="30,68,0,0" VerticalAlignment="Top"  IsThreeState="False"  Checked="cbInsertSpaceInParameterlistParentheses_Checked" />

    </Grid>
</UserControl>
